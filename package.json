    "rollup": "^0.52.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.8.4",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
